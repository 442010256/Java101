import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		System.out.println("ente a number");
	Scanner scan = new Scanner(System.in);
	int var = scan.nextInt();   
	switch(var%2){
	case 0:
	System.out.println("even");
    break;   
	default:
	System.out.println("odd");
	}    
	}
}
